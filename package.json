{
  "name": "@rob-long/angular-react",
  "version": "1.0.2",
  "description": "A package for integrating AngularJS with React",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack serve --mode development",
    "start": "http-server ./dist -p 80"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,json,md}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "keywords": [
    "angular",
    "react",
    "integration"
  ],
  "author": "Robert Long",
  "license": "ISC",
  "dependencies": {
    "@mavrck-inc/react-modules": "file:mavrck-inc-react-modules-0.7.17.tgz",
    "@rob-long/app-bridge": "^1.1.0",
    "angular": "^1.6.9",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@types/angular": "^1.8.9",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.0",
    "http-server": "^14.1.1",
    "install": "^0.13.0",
    "npm": "^10.8.1",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rob-long/angular-react.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com/"
  }
}
